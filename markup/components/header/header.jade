mixin header(data)
    .header(class!=data).not-styleLink
        .header__mobile.hidden-lg-up
            .header__mobile-nav
                .header__mobile-nav-section.header__mobile-nav-section_btn
                    .mobile-list-anim
                        input(type="checkbox", id="header-bottom-toggleMenuMbl")
                        label.menuTopMobel(for="header-bottom-toggleMenuMbl")
                            span.menu-mobile
                                span.menu-mobile-hamburger.one
                                span.menu-mobile-hamburger.two
                                span.menu-mobile-hamburger.three

                .header__mobile-nav-section
                    a(href="#").header__mobile-logo: +img('assets/header/logo-mobile', 'png')
                .header__mobile-nav-section.header__mobile-nav-section_btn.text-center
                    a(href="#").header__mobile-link: +icon('phone')
                .header__mobile-nav-section.header__mobile-nav-section_btn.text-center
                    a(href="#").header__mobile-link: +icon('search')

            .header__mobile-modal
                ul.header__mobile-menu.not-styleLink
                    li.active: a(href="#") Меню
                    li: a(href="#") Каталог
                    li: a(href="#") Услуги
                    li: a(href="#") О компании
                    li: a(href="#") Наши объекты
                    li: a(href="#") Контакты
                br
                br
                .header__mobile-listBtn
                    .header__mobile-listBtn-section
                        a(href="#").header__mobile-btn #[i.s-header__mobile-btn-1]ЗАКАЗАТЬ ЗВОНОК
                    .header__mobile-listBtn-section
                        a(href="#").header__mobile-btn #[i.s-header__mobile-btn-2]РАСЧЁТ МАТЕРИАЛА


        .header__desktop.hidden-md-down
            .header__top
                .container
                    .header__top-table
                        .header__top-table-left.hidden-md-down
                            a(href="#").header__logo: +img('assets/header/logo', 'png')
                        .header__top-table-right
                            .header__nav

                                ul.header__nav-list
                                    li: a(href="#").color-white: span.color-white__title Главная страница
                                    li: a(href="#").color-white: span.color-white__title О компании
                                    li: a(href="#").color-white: span.color-white__title Каталог продукции
                                    li: a(href="#").color-white: span.color-white__title Услуги
                                    li: a(href="#").color-white: span.color-white__title Наши объекты
                                    li: a(href="#").color-white: span.color-white__title Контакты

                            .header__top-container
                                .header__top-container-search
                                    div.header__top-container-search-linksListBlock
                                        .header__top-container-search-linksList
                                            div.hidden-lg-up
                                                a(href="#").header__logo: +img('assets/header/logo', 'png')
                                            div.hidden-xs-down
                                                a(href="#").link-icon.color-white
                                                    span.link-icon__icon: i.s-email
                                                    span.link-icon__title: span.color-white__title_viewBorder.color-white__title 2383886@mail.ru
                                                br
                                                span.link-icon
                                                    span.link-icon__icon: i.s-date
                                                    span.link-icon__title пн-сб: 09:00 - 19:00

                                            div
                                                .link-icon
                                                    .link-icon__icon: i.s-phone
                                                    .link-icon__title
                                                        a(href="#").link-icon__title-link.color-white: span.color-white__title +7 (351) 777-47-08
                                                        br
                                                        a(href="#").link-icon__title-link.color-white: span.color-white__title +7 (351) 238-38-86
                                    .header__search.hidden-md-down
                                        input(placeholder="Поиск по сайту").header__search-input
                                        button.header__search-btn ПОИСК


                                .header__top-container-btn
                                    button.header__top-btn
                                        span.header__top-btn-icon: i.s-advice
                                        span.header__top-btn-title ЗАКАЗАТЬ ЗВОНОК
                                    button.header__top-btn
                                        span.header__top-btn-icon: i.s-profile
                                        span.header__top-btn-title РАСЧЁТ МАТЕРИАЛА
            .header__bottom.hidden-md-down
                .container
                    .header__category
                        - var listCatNav = []
                        .header__category-list
                            -for(var i = 0; i<2; i++)
                                each categoryRow, key in header.categoryList
                                    - listCatNav[key] = categoryRow.listNav
                                    .header__category-list-section
                                        a(href="#", data-block="#navList-"+key).header__category-item
                                            span.header__category-item-img(class=categoryRow.imgClassName)
                                                span.header__category-item-img-default: +img('content/category-list/'+categoryRow.imgName, 'png')
                                                span.header__category-item-img-hover: +img('content/category-list/'+categoryRow.imgName+'-hover', 'png')
                                            span.header__category-item-title!= categoryRow.title

                        each navList, key in listCatNav
                            .header__category-nav(id='navList-'+key)
                                ul
                                    each navListRow in navList
                                        li: a(href="#").color-blue: span.color-blue__title= navListRow





























